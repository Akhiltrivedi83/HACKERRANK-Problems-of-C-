#include<iostream>
using namespace std;
int fibonacci(int n)
{
    int i,c,a=0,b=1;
    for(i=2;i<=n;i++)
    {
        c=a+b;
        cout<<c;
        a=b;
        b=c;
    }
    
}
int main()
{
    int n;
    cout<<"Enter the term of the series: ";
    cin>>n;
    cout<<"0","1";
   
    fibonacci(n);
    
    return 0;
}
