#include<iostream>
using namespace std;
int main()
{
    int x;
    cout<<"Enter the year: ";
    cin>>x;
    if(x%4==0)
    {
        if(x%100!=0||x%400==0)
        {
            cout<<"It is a leap year";
        }
        else
        {
            cout<<"not a leap year";
        }
        
    }
    else
    {
        cout<<"not a leap year";
    }
 return 0;   
}
