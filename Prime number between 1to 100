#include<iostream>
using namespace std;
int prime(int x)
{
    int i;
    for(i=2;i<x;i++)
    {
        if(x%i==0)
        return 0;
    }
    return 1;
}
int main()
{
    int n=100,i,ret;
    cout<<"Enter the number: ";
    for(i=2;i<=n;i++)
    {
        ret=prime(i);
    if(ret==1)
    {
        cout<<i<<" ";
    }
    }
    return 0;


}
