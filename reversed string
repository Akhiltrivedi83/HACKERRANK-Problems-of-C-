#include<iostream>
using namespace std;
int main()
{
    string ch;
    int i,count=0;
    getline(cin,ch);
    cout<<"The original string is: ";
    cout<<ch<<endl;
    for(i=0;ch[i]!='\0';i++)
    {
        count+=1;
    }
   // cout<<"The number of words in string are: "<<count<<"\n";
   cout<<"The reversed string is: ";
    for(i=count;i>=0;i--)
    {
        cout<<ch[i];
    }
    return 0;

}
